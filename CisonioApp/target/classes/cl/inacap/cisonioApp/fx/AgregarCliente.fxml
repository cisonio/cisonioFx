<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>

<fx:root fx:id="agregarClienteVBox" type="VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label styleClass="ms-font-l" text="Agregar Cliente" wrapText="true" />
      <FlowPane hgap="15.0" prefHeight="0.0" prefWidth="822.0" vgap="15.0">
         <children>
            <Label styleClass="ms-font-m" text="Nombre" />
            <TextField fx:id="nombreTxt" />
            <Label styleClass="ms-font-m" text="Telefono" />
            <TextField fx:id="telefonoTxt" />
            <Button fx:id="agregarPagoBtn" mnemonicParsing="false" onAction="#validar" style="-fx-background-color: -blue-jeans;" styleClass="ms-font-m-plus" text="Agregar" textFill="WHITE" />
         </children>
      </FlowPane>
   </children>
   <stylesheets>
      <URL value="@../../../../styles/Components.css" />
      <URL value="@../../../../styles/Colores.css" />
      <URL value="@../../../../styles/Fonts.css" />
   </stylesheets>
</fx:root>
